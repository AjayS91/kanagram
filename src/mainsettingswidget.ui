            <property name="name">
                <cstring>kcfg_newWordOnAnswer</cstring>
            </property>
            <property name="text">
                <string>Display new word on correct answer</string>
            </property>
        </widget>
        <widget class="QCheckBox" row="3" column="0" rowspan="1" colspan="2">
            <property name="name">
                <cstring>kcfg_useSounds</cstring>
            </property>
            <property name="text">
                <string>Use sounds</string>
            </property>
        </widget>
